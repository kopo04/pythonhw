#include <stdio.h>

int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);

    if (a > b && a % b == 0){
        printf("1번. %d", a / b);
    }
    else if (a > b && a % b != 0) {
        printf("2번. %d", a % b);
    }
    else if (a < b) {
        printf("3번. %d", a + b);
    }
    else {
        printf("4번. %d %d", a, b);
    }
    return 0;
}
