#include <stdio.h>

int main(void)
{
    int A, B;
    scanf("%d %d", &A, &B);

    if (A > B && A % B == 0){
        printf("1번. %d", A / B);
    }
    else if (A > B && A % B != 0) {
        printf("2번. %d", A % B);
    }
    else if (A < B) {
        printf("3번. %d", A + B);
    }
    else {
        printf("4번. %d %d", A, B);
    }
    return 0;
}
